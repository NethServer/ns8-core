{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>NethServer/.github:ns8-automerge"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "build-image.sh"
      ],
      "matchStrings": [
        "https://github\\.com/(?<depName>.+?)/releases/download/(?<currentValue>.+?)/logcli-linux-amd64\\.zip"
      ],
      "datasourceTemplate": "github-tags"
    }
  ]
}
