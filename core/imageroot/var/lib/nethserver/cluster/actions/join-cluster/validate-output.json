{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "join-cluster output",
    "$id": "http://schema.nethserver.org/cluster/join-cluster-output.json",
    "description": "",
    "examples": [
        {
            "node_id": 3,
            "ip_address": "10.5.4.3"
        }
    ],
    "type": "object",
    "required": [
        "node_id",
        "task_id"
    ],
    "properties": {
        "node_id": {
            "title": "Node identifier",
            "description": "Allocated identifier of the new node",
            "type": "integer",
            "minimumExclusive": 1
        },
        "task_id": {
            "title": "join-node task ID",
            "description": "Identifier of the join-node concurrent task",
            "type": "string",
            "minLength": 1
        }
    }
}
